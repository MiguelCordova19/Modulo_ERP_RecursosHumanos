CREATE PROCEDURE public.sp_obtener_menus_activos(INOUT resultado refcursor DEFAULT 'menu_cursor'::refcursor)
    LANGUAGE plpgsql
    AS $$
BEGIN
    OPEN resultado FOR
    SELECT 
        m.Menu_Id,
        m.Menu_Ruta,
        m.Menu_Nombre,
        m.Menu_Icon,
        m.Menu_Estado,
        m.menu_padre,
        m.menu_nivel,
        m.menu_posicion
    FROM RRHH_MMENU m
    WHERE m.Menu_Estado = 1
    ORDER BY m.menu_posicion ASC;
END;
$$;


